<p id="notice"><%= notice %></p>

<h1>Listing Vagas</h1>

<table>
  <thead>
    <tr>
    
      <th>Cargo</th>
      <th>Salario</th>
      <th>Cargahoraria</th>
      <th>Email</th>
      <th>Telefone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vagas.each do |vaga| %>
      <tr>
       
        <td><%= vaga.cargo %></td>
        <td><%= vaga.salario %></td>
        <td><%= vaga.cargaHoraria %></td>
        <td><%= vaga.email %></td>
        <td><%= vaga.telefone %></td>
        <td><%= link_to 'Show', vaga %></td>
        <td><%= link_to 'Edit', edit_vaga_path(vaga) %></td>
        <td><%= link_to 'Destroy', vaga, method: :delete, data: { confirm: 'Tem xerteza de que deseja excluir?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nova Vaga', new_vaga_path %>
